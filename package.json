{
  "name": "press-office",
  "main": "index.js",
  "version": "1.0.0",
  "scripts": {
    "dev": "expo start -c",
    "dev:web": "expo start -c --web",
    "dev:android": "expo start -c --android",
    "android": "expo start -c --android",
    "ios": "expo start -c --ios",
    "web": "expo start -c --web",
    "clean": "rm -rf .expo node_modules",
    "postinstall": "npx tailwindcss -i ./global.css -o ./node_modules/.cache/nativewind/global.css",
    "dev:client": "expo start --dev-client -c",
    "build:dev:ios": "eas build --profile development --platform ios",
    "build:preview:ios": "eas build --profile preview --platform ios",
    "build:prod:ios": "eas build --profile production --platform ios",
    "build:dev:android": "eas build --profile development --platform android",
    "build:preview:android": "eas build --profile preview --platform android",
    "build:prod:android": "eas build --profile production --platform android",
    "test": "jest --watchAll"
  },
  "dependencies": {
    "@faker-js/faker": "^9.7.0",
    "@gorhom/bottom-sheet": "^4.6.4",
    "@hookform/resolvers": "^4.1.3",
    "@nozbe/simdjson": "^3.9.4",
    "@nozbe/sqlite": "^3.46.0",
    "@nozbe/watermelondb": "^0.27.1",
    "@react-navigation/native": "^7.0.0",
    "@rn-primitives/accordion": "^1.1.0",
    "@rn-primitives/alert-dialog": "^1.1.0",
    "@rn-primitives/avatar": "~1.1.0",
    "@rn-primitives/label": "^1.1.0",
    "@rn-primitives/portal": "~1.1.0",
    "@rn-primitives/progress": "~1.1.0",
    "@rn-primitives/radio-group": "^1.1.0",
    "@rn-primitives/separator": "^1.1.0",
    "@rn-primitives/slot": "~1.1.0",
    "@rn-primitives/table": "^1.1.0",
    "@rn-primitives/tabs": "^1.1.0",
    "@rn-primitives/tooltip": "~1.1.0",
    "@rn-primitives/types": "~1.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "expo": "^52.0.25",
    "expo-build-properties": "~0.13.2",
    "expo-dev-client": "~5.0.17",
    "expo-linking": "~7.0.5",
    "expo-navigation-bar": "~4.0.9",
    "expo-router": "4.0.20",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.9",
    "lucide-react-native": "^0.378.0",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.55.0",
    "react-native": "0.76.8",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-web": "~0.19.13",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "3.3.5",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2",
    "zustand": "^4.5.6"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.3.12",
    "jest": "~29.7.0",
    "jest-expo": "~52.0.6",
    "react-test-renderer": "^18.3.1",
    "typescript": "^5.3.3"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg|@rn-primitives/.*)"
    ]
  },
  "private": true
}
