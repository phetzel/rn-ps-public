{
  "name": "press-office",
  "main": "index.js",
  "version": "1.0.0",
  "scripts": {
    "dev": "expo start -c",
    "dev:web": "expo start -c --web",
    "dev:android": "expo start -c --android",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "android": "expo start -c --android",
    "ios": "expo start -c --ios",
    "web": "expo start -c --web",
    "clean": "rm -rf .expo node_modules",
    "postinstall": "npx tailwindcss -i ./global.css -o ./node_modules/.cache/nativewind/global.css",
    "prepare": "husky",
    "dev:client": "expo start --dev-client -c",
    "build:dev:ios": "eas build --profile development --platform ios",
    "build:preview:ios": "eas build --profile preview --platform ios",
    "build:prod:ios": "eas build --profile production --platform ios",
    "build:dev:android": "eas build --profile development --platform android",
    "build:preview:android": "eas build --profile preview --platform android",
    "build:prod:android": "eas build --profile production --platform android",
    "test": "jest --watchAll",
    "test:ci": "jest --watchAll=false --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "gen-situation": "tsx scripts/run-gen-situation.ts",
    "e2e": "maestro test e2e/maestro/suites/smoke.yaml",
    "e2e:ios": "maestro test --platform=ios e2e/maestro/suites/smoke.yaml",
    "e2e:android": "maestro test --platform=android e2e/maestro/suites/smoke.yaml"
  },
  "dependencies": {
    "@amplitude/analytics-react-native": "^1.5.16",
    "@amplitude/plugin-session-replay-react-native": "^0.4.3",
    "@faker-js/faker": "^9.7.0",
    "@gorhom/bottom-sheet": "^4.6.4",
    "@hookform/resolvers": "^4.1.3",
    "@nozbe/simdjson": "^3.9.4",
    "@nozbe/sqlite": "^3.46.0",
    "@nozbe/watermelondb": "^0.27.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/native": "^7.0.0",
    "@rn-primitives/accordion": "^1.1.0",
    "@rn-primitives/alert-dialog": "^1.1.0",
    "@rn-primitives/avatar": "~1.1.0",
    "@rn-primitives/label": "^1.1.0",
    "@rn-primitives/portal": "~1.1.0",
    "@rn-primitives/progress": "~1.1.0",
    "@rn-primitives/radio-group": "^1.1.0",
    "@rn-primitives/select": "^1.2.0",
    "@rn-primitives/separator": "^1.1.0",
    "@rn-primitives/slot": "~1.1.0",
    "@rn-primitives/table": "^1.1.0",
    "@rn-primitives/tabs": "^1.1.0",
    "@rn-primitives/tooltip": "~1.1.0",
    "@rn-primitives/types": "~1.1.0",
    "@sentry/react-native": "~6.10.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "expo": "^52.0.25",
    "expo-build-properties": "~0.13.2",
    "expo-dev-client": "~5.0.20",
    "expo-linking": "~7.0.5",
    "expo-navigation-bar": "~4.0.9",
    "expo-router": "4.0.20",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.9",
    "expo-tracking-transparency": "~5.1.1",
    "lucide-react-native": "^0.378.0",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.55.0",
    "react-native": "0.76.8",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-google-mobile-ads": "^15.4.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-web": "~0.19.13",
    "sentry-expo": "~7.0.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "3.3.5",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2",
    "zustand": "^4.5.6"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.15",
    "@types/react": "~18.3.12",
    "better-sqlite3": "^11.10.0",
    "dotenv": "^17.2.0",
    "eslint": "^9.39.1",
    "eslint-config-expo": "^10.0.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "~29.7.0",
    "jest-expo": "~52.0.6",
    "lint-staged": "^15.5.2",
    "openai": "^5.12.2",
    "prettier": "^3.6.2",
    "react-test-renderer": "^18.3.1",
    "tsx": "^4.19.4",
    "typescript": "^5.3.3",
    "zod-to-json-schema": "^3.24.6"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/support/jest-setup.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/support/*"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg|@rn-primitives/.*|@nozbe/watermelondb|better-sqlite3|nativewind|react-native-css-interop)"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "private": true
}
